# JetFriend API Configuration
# Copy this file to .env and replace with your actual API keys

# OpenAI GPT-4o API Key (required for AI chat functionality)
OPENAI_API_KEY=your-openai-api-key-here

# Google Places API Key (required for location and restaurant search)
GOOGLE_PLACES_API_KEY=your-google-places-api-key-here

# Optional: Set to true for debug mode
DEBUG=false

# Optional: Custom port (default: 5000)
PORT=5000
